{
	"compileOnSave": true,
	"compilerOptions":
	{
		"lib": [ "es2020" ],
		"target": "es2020",
		"module": "es2020",
		"moduleResolution": "nodenext",
		"strict": true,
		"outDir": "lib",
		"declaration": true,
		"declarationMap": true
	},
	"include": [ "src" ],
	"typedocOptions": {
		"entryPoints": [
			"src/index.ts"
		],
		"out": "docs",
		"excludePrivate": true,
		"excludeProtected": true,
		"excludeNotDocumented": true,
		"excludeInternal": true,
		"includeVersion": true,
		"gitRemote": "origin",
		"hideGenerator": false,
		"titleLink": "https://versatica.github.io/rtp.js",
		"treatWarningsAsErrors": true,
		"cacheBust": false,
		"categorizeByGroup": false,
		"categoryOrder": [ "RTP", "RTCP", "Helpers", "*" ],
		"navigationLinks": {
			"GitHub": "https://github.com/versatica/rtp.js",
			"NPM": "https://www.npmjs.com/package/rtp.js"
		},
		"navigationLinks": {
			"GitHub": "https://github.com/versatica/rtp.js"
		}
	}
}
